{
  "name": "anna-backend",
  "version": "1.4.1",
  "description": "Backend for ANNA intranet",
  "main": "app.js",
  "author": "IpsaOne DevTeam",
  "private": true,
  "license": "ISC",
  "dependencies": {
    "async": "^3.2.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "connect-redis": "^5.0.0",
    "cors": "^2.8.4",
    "deep-equal": "^2.0.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.0.0",
    "express-boom": "^3.0.0",
    "express-rate-limit": "^5.1.3",
    "express-request-id": "^1.4.0",
    "express-session": "^1.17.1",
    "find-root": "^1.1.0",
    "flatted": "^3.1.0",
    "glob": "^7.1.6",
    "hasha": "^5.2.1",
    "helmet": "^4.1.1",
    "inquirer": "^7.3.3",
    "joi": "^17.2.1",
    "mailgun-js": "^0.22.0",
    "marked": "^1.2.0",
    "minimatch": "^3.0.4",
    "mmmagic": "^0.5.3",
    "moment": "^2.29.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "mv": "^2.1.1",
    "mysql": "^2.18.1",
    "mysql2": "^2.2.5",
    "nodemailer": "^6.4.13",
    "redis": "^3.0.2",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "rimraf": "^3.0.2",
    "sequelize": "^6.3.5",
    "sqlite3": "^5.0.0",
    "superagent": "^6.1.0",
    "superagent-throttle": "^1.0.1",
    "trello-node-api": "0.0.9",
    "uuid": "^8.3.1",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0",
    "zip-a-folder": "0.0.12"
  },
  "devDependencies": {
    "ava": "^3.13.0",
    "chance": "^1.1.7",
    "eslint": "^7.18.0",
    "eslint-plugin-promise": "^4.1.1",
    "jsdoc": "^3.6.6",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "sequelize-cli": "^6.2.0",
    "sequelize-mock": "^0.10.2",
    "supertest": "^5.0.0"
  },
  "node_deb": {
    "entrypoints": {
      "daemon": "./src/app.js"
    }
  },
  "nyc": {
    "check-coverage": true,
    "cache": true,
    "lines": 80,
    "statements": 80,
    "functions": 80,
    "branches": 50,
    "reporter": [
      "html",
      "text-summary"
    ],
    "exclude": [
      "src/modules/**/test/*.js",
      "src/modules/logs"
    ]
  },
  "ava": {
    "files": [
      "src/modules/**/test/*.js",
      "test/*.js"
    ],
    "cache": true,
    "failFast": true,
    "verbose": true,
    "@ava/babel": {},
    "concurrency": 8
  },
  "scripts": {
    "start": "node ./src/app.js",
    "test": "bash ./test/test.sh",
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "init-db": "npm run migrate && npm run seed",
    "clear-db": "sequelize db:migrate:undo:all && rm -rf ./storage",
    "lint": "eslint --cache --fix --max-warnings 50 ./modules/",
    "lint-nofix": "eslint --cache --max-warnings 50 ./modules/",
    "debug": "nodemon -L --inspect=0.0.0.0:9229 ./src/app.js",
    "coverage": "http-server ./coverage -p 8080 -c-1 -o",
    "load": "node --prof --no-logfile-per-isolate --logfile=./logs/v8_raw.log ./test/load.js && node --prof-process ./logs/v8_raw.log > ./logs/v8_proc.log",
    "linux-setup-ssh": "vagrant ssh-config > ~/.ssh/config",
    "mirror-storage": "node ./devops/mirror_storage.js"
  },
  "engines": {
    "node": ">=13"
  }
}
