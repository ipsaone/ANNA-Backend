{
  "name": "anna-backend",
  "version": "1.2.0",
  "description": "Backend for ANNA intranet",
  "main": "app.js",
  "author": "IpsaOne DevTeam",
  "private": true,
  "license": "ISC",
  "dependencies": {
    "async": "^3.1.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "connect-redis": "^4.0.3",
    "cors": "^2.8.4",
    "deep-equal": "^1.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.0.0",
    "express-boom": "^3.0.0",
    "express-rate-limit": "^5.0.0",
    "express-request-id": "^1.4.0",
    "express-session": "^1.17.0",
    "find-root": "^1.1.0",
    "flatted": "^2.0.1",
    "glob": "^7.1.4",
    "hasha": "^5.1.0",
    "helmet": "^3.21.1",
    "inquirer": "^7.0.0",
    "joi": "^14.3.1",
    "marked": "^0.7.0",
    "minimatch": "^3.0.4",
    "mmmagic": "^0.5.3",
    "moment": "^2.24.0",
    "morgan": "^1.9.0",
    "multer": "^1.4.2",
    "mv": "^2.1.1",
    "mysql": "^2.17.1",
    "mysql2": "^1.5.2",
    "redis": "^2.8.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "rimraf": "^3.0.0",
    "sequelize": "^5.19.7",
    "sqlite3": "^4.1.0",
    "superagent": "^5.1.0",
    "superagent-throttle": "^1.0.1",
    "trello-node-api": "0.0.7",
    "uuid": "^3.3.3",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.2.1",
    "zip-a-folder": "0.0.9"
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "chance": "^1.1.3",
    "clinic": "^4.1.0",
    "eslint": "^6.5.1",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-promise": "^4.1.1",
    "jsdoc": "^3.6.2",
    "nodemon": "^1.19.4",
    "nyc": "^14.1.1",
    "sequelize-cli": "^5.5.1",
    "sequelize-mock": "^0.10.2",
    "supertest": "^4.0.2"
  },
  "node_deb": {
    "entrypoints": {
      "daemon": "./src/app.js"
    }
  },
  "nyc": {
    "check-coverage": true,
    "cache": true,
    "lines": 80,
    "statements": 80,
    "functions": 80,
    "branches": 50,
    "reporter": [
      "html",
      "text-summary"
    ],
    "exclude": [
      "src/modules/**/test/*.js",
      "src/modules/logs"
    ]
  },
  "ava": {
    "files": [
      "src/modules/**/test/*.js",
      "test/*.js"
    ],
    "cache": true,
    "failFast": true,
    "verbose": true,
    "babel": {},
    "concurrency": 8
  },
  "scripts": {
    "start": "node ./src/app.js",
    "test": "bash ./test/test.sh",
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "init-db": "npm run migrate && npm run seed",
    "clear-db": "sequelize db:migrate:undo:all && rm -rf ./storage",
    "lint": "eslint --cache --fix --max-warnings 50 ./modules/",
    "lint-nofix": "eslint --cache --max-warnings 50 ./modules/",
    "debug": "nodemon -L --inspect=0.0.0.0:9229 ./src/app.js",
    "coverage": "http-server ./coverage -p 8080 -c-1 -o",
    "load": "node --prof --no-logfile-per-isolate --logfile=./logs/v8_raw.log ./test/load.js && node --prof-process ./logs/v8_raw.log > ./logs/v8_proc.log",
    "deploy": "rm -rf ./node_modules/ && npm install && npm migrate",
    "linux-setup-ssh": "vagrant ssh-config > ~/.ssh/config",
    "mirror-storage": "node ./devops/mirror_storage.js"
  },
  "engines": {
    "node": ">=12"
  }
}
